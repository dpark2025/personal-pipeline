{
  "id": "test_disk_space",
  "title": "Test Disk Space Runbook",
  "version": "1.0",
  "description": "Test runbook for unit testing",
  "triggers": [
    "disk_usage_critical",
    "filesystem_full"
  ],
  "severity_mapping": {
    "critical": "critical",
    "high": "high"
  },
  "decision_tree": {
    "id": "test_decision_tree",
    "name": "Test Decision Tree",
    "branches": [
      {
